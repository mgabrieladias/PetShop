	public abstract String descricao();
	public abstract double preco();

	
	public class Bamho extends Servico {

		@Override
		public String descricao() {
			// TODO Auto-generated method stub
			return "Servi√ßo de banho";
		}

		@Override
		public double preco() {
			// TODO Auto-generated method stub
			return 20.0;
		}
		
		
	}
	
}
