package pacote1;

import java.util.Date;

public class Animal {
	
	public String nome;
	public String especie;
	public String raca;
	public Date DataNascimento;
	public String proprietario;
	
	
	public Animal(String nome, String especie, String raca, Date dataNascimento, String proprietario) {
		super();
		this.nome = nome;
		this.especie = especie;
		this.raca = raca;
		DataNascimento = dataNascimento;
		this.proprietario = proprietario;
	}
	
	
	
	
	

	

}
