package pacote2;

public class Brinquedo extends Produto {

	public Brinquedo(String nome, String categoria, double preco, int estoque) {
		super(nome, categoria, preco, estoque);
		
	}
	
	public String nome() {

		return "Bolinha";
	}

	
	public String categoria() {

		return "Brinquedo";
	}

	public double preco() {

		return 5.0;
	}
	
}
